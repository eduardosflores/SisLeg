<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AgendaSesion" table="agendaSesion">
    <indexes>
      <index name="agendaSesion_expediente_idx" columns="idExpediente"/>
      <index name="agendaSesion_sesion_idx" columns="idSesion"/>
      <index name="agendaSesion_estadoAgendaSesion_idx" columns="idEstadoAgendaSesion"/>
    </indexes>
    <id name="id" type="integer" column="idAgendaSesion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="aFavor" type="smallint" column="aFavor" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="enContra" type="smallint" column="enContra" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="abstenciones" type="smallint" column="abstenciones" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="estadoAgendaSesion" target-entity="EstadoAgendaSesion" fetch="LAZY">
      <join-columns>
        <join-column name="idEstadoAgendaSesion" referenced-column-name="idEstadoAgendaSesion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="expediente" target-entity="Expediente" fetch="LAZY">
      <join-columns>
        <join-column name="idExpediente" referenced-column-name="idExpediente"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sesion" target-entity="Sesion" inversed-by="ordenDelDia" fetch="LAZY">
      <join-columns>
        <join-column name="idSesion" referenced-column-name="idSesion"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

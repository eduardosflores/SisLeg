<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Menu" table="menu">
    <id name="id" type="smallint" column="idMenu">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="menu" type="string" column="menu" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
     <field name="abreviacion" type="string" column="abreviacion" length="70" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-many field="items" mapped-by="menu" target-entity="MenuItem"/>
  </entity>
</doctrine-mapping>

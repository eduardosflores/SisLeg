<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Dictamen" table="dictamen">
    <indexes>
      <index name="dictamen_tipoDictamen_idx" columns="idTipoDictamen"/>
    </indexes>
    <unique-constraints>
    	<unique-constraint columns="idProyectoRevision" name="UNIQ_dictamen_proyectoRevision_idx"/>
    </unique-constraints>
    <id name="id" type="integer" column="idDictamen">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="usuarioCreacion" type="string" column="usuarioCreacion" length="70" nullable="false">
    </field>
    <field name="fechaCreacion" type="datetime" column="fechaCreacion" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field> 
    <many-to-one field="revisionDictamen" target-entity="ProyectoRevision">
    	<join-columns>
    		<join-column name="idProyectoRevision" referenced-column-name="idProyectoRevision"/>
    	</join-columns>
    </many-to-one>
    <many-to-one field="tipoDictamen" target-entity="TipoDictamen" fetch="LAZY">
      <join-columns>
        <join-column name="idTipoDictamen" referenced-column-name="idTipoDictamen"/> 
      </join-columns>
    </many-to-one> 
  </entity>  
</doctrine-mapping>

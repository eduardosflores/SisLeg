<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TipoPerfil" table="tipoPerfil">
    <id name="id" type="smallint" column="idTipoPerfil">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipoPerfil" type="string" column="tipoPerfil" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="roles" target-entity="Rol" fetch="EAGER">
      <join-table name="tipoPerfil_rol">
          <join-columns>
              <join-column name="idTipoPerfil" referenced-column-name="idTipoPerfil" />
          </join-columns>
          <inverse-join-columns>
              <join-column name="idRol" referenced-column-name="idRol"/>
          </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
   
</doctrine-mapping>

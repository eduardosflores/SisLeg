<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\DictamenArticulado">
     <field name="textoArticulado" type="json_array" column="textoArticulado" nullable="false">
    </field>
    <many-to-one field="tipoDictamen" target-entity="TipoProyecto">
    	<join-columns>
    		<join-column name="IdTipoDictamen" referenced-column-name="idTipoProyecto"/>
    	</join-columns>
    </many-to-one>
  </entity>  
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ExpedienteSesion" table="expedienteSesion">
    <indexes>
      <index name="expedienteSesion_expediente_idx" columns="idExpediente"/>
      <index name="expedienteSesion_sesion_idx" columns="idSesion"/>
      <index name="expedienteSesion_estadoExpedienteSesion_idx" columns="idEstadoExpedienteSesion"/>
      <index name="expedienteSesion_tipoExpedienteSesion_idx" columns="idTipoExpedienteSesion"/>
    </indexes>
    <id name="id" type="integer" column="idExpedienteSesion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ordenSesion" column="ordenSesion" type="smallint">
    </field>
    <field name="texto" type="text" nullable="false">
    </field>
    <field name="aFavor" type="smallint" column="aFavor" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="enContra" type="smallint" column="enContra" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="abstenciones" type="smallint" column="abstenciones" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="tipoExpedienteSesion" target-entity="TipoExpedienteSesion">
    	<join-columns>
    		<join-column name="idTipoExpedienteSesion" referenced-column-name="idTipoExpedienteSesion"/>
    	</join-columns>
    </many-to-one>
    <many-to-one field="estadoExpedienteSesion" target-entity="EstadoExpedienteSesion" fetch="LAZY">
      <join-columns>
        <join-column name="idEstadoExpedienteSesion" referenced-column-name="idEstadoExpedienteSesion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="expediente" target-entity="Expediente" fetch="LAZY">
      <join-columns>
        <join-column name="idExpediente" referenced-column-name="idExpediente"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sesion" target-entity="Sesion" fetch="LAZY">
      <join-columns>
        <join-column name="idSesion" referenced-column-name="idSesion"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

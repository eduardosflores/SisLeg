<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Bloque" table="bloque" repository-class="AppBundle\Repository\BloqueRepository">
    <id name="id" type="smallint" column="idBloque">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bloque" type="string" column="bloque" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <field name="usuarioCreacion" type="string" length="70" column="usuarioCreacion" nullable="false">
    </field>
    <field name="fechaCreacion" type="datetime" column="fechaCreacion" nullable="false">
    </field>
    <field name="usuarioModificacion" type="string" length="70" column="usuarioModificacion" nullable="true">
    </field>
    <field name="fechaModificacion" type="datetime" column="fechaModificacion" nullable="true">
    </field>
    <one-to-many field="concejales" mapped-by="bloque" target-entity="PerfilLegislador"></one-to-many>
  </entity>
</doctrine-mapping>
	
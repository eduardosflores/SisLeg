<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TipoProyecto" table="tipoProyecto">
     <indexes>
      <index name="tipoproyecto_tipoexpediente_idx" columns="idTipoExpediente"/>
    </indexes>
    <id name="id" type="smallint" column="idTipoProyecto">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipoProyecto" type="string" column="tipoProyecto" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="tipoExpediente" target-entity="TipoExpediente" fetch="LAZY">
      <join-columns>
        <join-column name="idTipoExpediente" referenced-column-name="idTipoExpediente"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

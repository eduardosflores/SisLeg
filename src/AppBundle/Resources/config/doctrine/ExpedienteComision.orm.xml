<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ExpedienteComision" table="expedienteComision" 
  		  repository-class="AppBundle\Repository\ExpedienteComisionRepository">
    <indexes>
      <index name="expedienteComision_expediente_idx" columns="idExpediente"/>
      <index name="expedienteComision_comision_idx" columns="idComision"/>
      <index name="expedienteComision_movimiento_idx" columns="idMovimiento"/>
    </indexes>
    <id name="id" type="integer" column="idExpedienteComision">
      <generator strategy="IDENTITY"/>
    </id>	
    <field name="anulado" type="boolean" column="anulado" nullable="false">
    </field>
    <field name="fechaCreacion" type="datetime" column="fechaCreacion" nullable="false"/>
    <field name="usuarioCreacion" type="string" column="usuarioCreacion" length="70" 
           nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaModificacion" type="datetime" column="fechaModificacion" nullable="true"/>
     <field name="usuarioModificacion" type="string" column="usuarioModificacion" length="70" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="paseOriginario" target-entity="Pase">
    	<join-columns>
    		<join-column name="idMovimiento" referenced-column-name="idMovimiento"/>
    	</join-columns>
    </many-to-one>
    <many-to-one field="comision" target-entity="Comision" inversed-by="expedientesAsignados">
      <join-columns>
        <join-column name="idComision" referenced-column-name="idComision"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="expediente" target-entity="Expediente" inversed-by="asignacionComisiones">
    	<cascade>
    		<cascade-persist/>
    	</cascade>
      <join-columns>
        <join-column name="idExpediente" referenced-column-name="idExpediente"/> 
      </join-columns>
    </many-to-one>
    <many-to-many field="dictamenesMayoria" target-entity="Dictamen" inversed-by="asignacionesPorMayoria">
    	<cascade>
	    	<cascade-persist/>
	    </cascade>
    	<join-table name="expedienteComision_dictamenesMayoria">
    		<join-columns>
    			<join-column name="idExpedienteComision" referenced-column-name="idExpedienteComision"/>
    		</join-columns>
    		<inverse-join-columns>
    			<join-column name="idDictamen" referenced-column-name="idDictamen"/>
    		</inverse-join-columns>
    	</join-table>
    </many-to-many>
    <many-to-many field="dictamenesPrimeraMinoria" target-entity="Dictamen" inversed-by="asignacionesPorPrimeraMinoria">
    	<cascade>
	    	<cascade-persist/>
	    </cascade>
    	<join-table name="expedienteComision_dictamenesPrimeraMinoria">
    		<join-columns>
    			<join-column name="idExpedienteComision" referenced-column-name="idExpedienteComision"/>
    		</join-columns>
    		<inverse-join-columns>
    			<join-column name="idDictamen" referenced-column-name="idDictamen"/>
    		</inverse-join-columns>
    	</join-table>
    </many-to-many>
    <many-to-many field="dictamenesSegundaMinoria" target-entity="Dictamen" inversed-by="asignacionesPorSegundaMinoria">
    	<cascade>
	    	<cascade-persist/>
	    </cascade>
    	<join-table name="expedienteComision_dictamenesSegundaMinoria">
    		<join-columns>
    			<join-column name="idExpedienteComision" referenced-column-name="idExpedienteComision"/>
    		</join-columns>
    		<inverse-join-columns>
    			<join-column name="idDictamen" referenced-column-name="idDictamen"/>
    		</inverse-join-columns>
    	</join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>

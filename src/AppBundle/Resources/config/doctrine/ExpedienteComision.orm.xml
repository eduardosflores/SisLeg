<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ExpedienteComision" table="expedienteComision">
    <indexes>
      <index name="expedienteComision_expediente_idx" columns="idExpediente"/>
      <index name="expedienteComision_comision_idx" columns="idComision"/>
    </indexes>
    <id name="id" type="integer" column="idExpedienteComision">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="aprobado" type="boolean" column="aprobado" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fechaAsignacion" type="datetime" column="fechaAsignacion" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="fechaAprobacion" type="datetime" column="fechaAprobacion" nullable="true"/>
    <many-to-one field="comision" target-entity="Comision" fetch="LAZY" inversed-by="expedientesAsignados">
      <join-columns>
        <join-column name="idComision" referenced-column-name="idComision"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="expediente" target-entity="Expediente" fetch="LAZY">
      <join-columns>
        <join-column name="idExpediente" referenced-column-name="idExpediente"/> 
      </join-columns>
    </many-to-one>
    <one-to-many field="dictamenes" target-entity="ExpedienteComisionDictamen" mapped-by="expedienteComision"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Oficina" table="oficina">
	<indexes>
		<index columns="idTipoOficina" name="oficina_tipoOficina_idx"></index>
	</indexes>
    <id name="id" type="integer" column="idOficina">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="oficina" type="string" column="oficina" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activa" type="boolean" column="activa" nullable="false"/>
    <field name="usuarioCreacion" type="string" length="70" column="usuarioCreacion" nullable="false">
    </field>
    <field name="fechaCreacion" type="datetime" column="fechaCreacion" nullable="false">
    </field>
    <field name="usuarioModificacion" type="string" length="70" column="usuarioModificacion" nullable="true">
    </field>
    <field name="fechaModificacion" type="datetime" column="fechaModificacion" nullable="true">
    </field>
    <many-to-one field="tipoOficina" target-entity="TipoOficina">
    	<join-columns>
    		<join-column name="idTipoOficina" referenced-column-name="idTipoOficina"/>
    	</join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TipoSesion" table="tipoSesion">
    <id name="id" type="smallint" column="idTipoSesion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipoSesion" type="string" column="tipoSesion" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="abreviacion" type="string" column="abreviacion" length="1" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

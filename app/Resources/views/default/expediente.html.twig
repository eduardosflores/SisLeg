			$(".chkSesion").bootstrapToggle("on");
			
			switch(parseInt(mostrar)){
			
				case 3:
						$(".ente-externo").addClass('hidden');
						$(".proyecto-concejales").addClass('hidden');
						$(".expediente-particular").removeClass('hidden');
